        "    pattern = r'@\\w+\\s?'\n",
        "    result = re.sub(pattern, '', text)\n",
        "    return result\n",
        "\n",
        "\n",
        "data['Text'] = data['Text'].apply(remove_usernames)\n",
        "data"
